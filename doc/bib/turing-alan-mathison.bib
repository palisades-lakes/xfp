  CODEN =        "AALEE5",
  DOI =          "<a href="http://dx.doi.org/10.1145/2402709.2402687">http://dx.doi.org/10.1145/2402709.2402687</a>",
  ISSN =         "1094-3641 (print), 1557-9476 (electronic)",
